public class staticpoly{

     public static void main(String []args){
       
        System.out.println("Area of a square : "+Calculate.area(2, 4)); 
  
        System.out.println("Area of a rectangle : "+Calculate.area(5.5, 6.3)); 
    }
}
class Calculate { 
  
   
    static int area(int a) 
    { 
        return a * a; 
    } 
  
    static double area(double a, double b) 
    { 
        return a * b; 
    } 
} 
  
